{{#if parent}}
    {{#with parent}}{{> breadcrumb}}{{/with}}
    <li class="breadcrumb-li">
        {{#if url}}
            <a href="{{relativeURL url}}" class="breadcrumb">{{name}}</a>
        {{else}}
            <span>{{name}}</span>
        {{/if}}
    </li>
{{else}}
    {{#if url}}
        <li class="no-after-slash">
            <a href="{{relativeURL url}}" >@docsvisin/webclient/</a>
        </li>
    {{/if}}
{{/if}}